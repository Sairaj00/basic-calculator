#include <stdio.h>
#include <cs50.h>
#include <string.h>

int main(void)
{
    float x = get_float("x:");
    float y = get_float("y:");

    char c= get_char ("+ or - or / or * :");
    if (c=='+')
    {
        printf("%f\n",x+y);
    }

    else if (c=='-')
    {
        printf("%f\n",x-y);
    }
    else if (c=='/')
    {
        printf("%f\n",x/y);
    }
    else if (c=='*')
    {
        printf("%f\n",x*y);
    }
    else{
        printf("no such type of operator is present here.\n");
    }
}
